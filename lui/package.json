{
  "name": "petercat-lui",
  "version": "0.1.9",
  "description": "A react library developed with dumi",
  "module": "dist/es/index.js",
  "types": "dist/es/index.d.ts",
  "browser": "dist/umd/petercat-lui.min.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "dumi dev",
    "build": "tailwind -c tailwind.config.js -i tailwind.css -o .dumi/tmp/tailwind.css && father build",
    "build:watch": "father dev",
    "docs:build": "dumi build",
    "prepare": "dumi setup",
    "doctor": "father doctor",
    "lint": "npm run lint:es && npm run lint:css",
    "lint:css": "stylelint \"{src,test}/**/*.{css,less}\"",
    "lint:es": "eslint \"{src,test}/**/*.{js,jsx,ts,tsx}\"",
    "prepublishOnly": "father doctor && npm run build"
  },
  "authors": [],
  "license": "MIT",
  "files": [
    "dist"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --write --no-error-on-unmatched-pattern"
    ],
    "*.{css,less}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --parser=typescript --write"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "dependencies": {
    "@ant-design/icons": "^5.3.5",
    "@ant-design/pro-chat": "^1.9.0",
    "@ant-design/pro-editor": "^1.1.1",
    "@babel/runtime": "^7.18.0",
    "antd": "^5.15.3",
    "axios": "^1.6.7",
    "classnames": "^2.5.1",
    "dotenv": "^16.4.5",
    "lodash": "^4.17.21",
    "lottie-react": "^2.4.0",
    "swr": "^2.2.5",
    "tailwindcss": "^3.4.1",
    "tailwindcss-scoped-preflight": "^3.2.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@types/lodash": "^4.14.202",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@umijs/lint": "^4.0.0",
    "@umijs/plugins": "^4.1.5",
    "dumi": "^2.3.5",
    "eslint": "^8.23.0",
    "father": "^4.1.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.0.0",
    "prettier-plugin-packagejson": "^2.2.18",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "stylelint": "^14.9.1"
  }
}
